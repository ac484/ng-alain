rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    // 合約附件 - 認證用戶可以讀寫刪除
    match /contracts/{contractId}/attachments/{fileName} {
      allow read, write, delete: if request.auth != null;
    }

    // 其他檔案 - 預設拒絕
    match /{allPaths=**} {
      allow read, write, delete: if false;
    }
  }
}
