{
  "name": "firebase-node-js-rest-api-example",
  "description": "A backend API example to be used as template for Firebase projects.",
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "emulator": "nodemon -e ts --exec \"npm run build && firebase emulators:start --only functions\"",
    "serve": "npm run build && firebase emulators:start --only functions",
    "deploy": "firebase deploy --only functions"
  },
  "keywords": [
    "firebase",
    "firestore",
    "authentication",
    "typescript",
    "api",
    "server",
    "backend"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/vision": "^5.3.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "firebase-admin": "^11.11.1",
    "firebase-functions": "^4.5.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
