{
    "mcpServers": {
      "angular-cli": {
        "command": "npx",
        "args": [
          "-y",
          "@talzach/mcp-angular-cli"
        ],
        "disabled": false,
        "autoApprove": [
          "generate",
          "build",
          "serve",
          "test",
          "lint",
          "e2e",
          "update"
        ]
      },
      "context7": {
        "command": "npx",
        "args": ["-y", "@upstash/context7-mcp@latest"],
        "autoApprove": [
          "resolve-library-id",
          "get-library-docs"
        ]
      },
      "filesystem": {
        "command": "npx",
        "args": [
          "-y",
          "@modelcontextprotocol/server-filesystem",
          "./src",
          "./custom_modes",
          "./memory-bank",
          "./functions"
        ],
        "disabled": false,
        "autoApprove": [
          "list_allowed_directories",
          "list_directory",
          "directory_tree",
          "create_directory",
          "write_file",
          "read_file",
          "edit_file",
          "delete_file",
          "move_file",
          "copy_file"
        ]
      },
      "memory": {
        "command": "npx",
        "args": [
          "-y",
          "@modelcontextprotocol/server-memory"
        ],
        "autoApprove": [
          "read",
          "write",
          "update",
          "delete",
          "clear"
        ]
      },
      "mcp-memory-bank": {
        "command": "uvx",
        "args": [
          "--from",
          "git+https://github.com/ipospelov/mcp-memory-bank",
